{
  "name": "backend-starter",
  "version": "1.0.0",
  "description": "Backend starter template",
  "main": "dist/server.js",
  "repository": "https://github.com/backmeupplz/backend-starter",
  "author": "backmeupplz <backmeupplz@gmail.com>",
  "license": "MIT",
  "private": false,
  "scripts": {
    "distribute": "yarn build-ts && yarn cpx-prod && node dist/server.js",
    "develop": "concurrently -i -k -p \"[{name}]\" -n \"Node,TypeScript,Copy-Views\" -c \"yellow.bold,cyan.bold,reset\" \"yarn watch-js\" \"yarn watch-ts\" \"yarn cpx-dev\"",
    "build-ts": "tsc --skipLibCheck",
    "watch-ts": "tsc -w --skipLibCheck",
    "watch-js": "nodemon dist/server.js",
    "cpx-dev": "cpx \"src/views/**\" dist/views --watch",
    "cpx-prod": "cpx \"src/views/**\" dist/views",
    "test": "yarn jest --forceExit",
    "watch": "webpack --watch"
  },
  "devDependencies": {
    "@babel/core": "^7.15.5",
    "@babel/preset-env": "^7.15.4",
    "@koa/cors": "^3.1.0",
    "@types/dotenv": "^8.2.0",
    "@types/jest": "^26.0.20",
    "@types/jsonwebtoken": "^8.5.2",
    "@types/koa": "^2.13.3",
    "@types/koa-bodyparser": "^4.3.1",
    "@types/lodash": "^4.14.168",
    "@types/node": "^15.12.5",
    "@types/sqlite3": "^3.1.7",
    "babel-loader": "^8.2.2",
    "concurrently": "^6.0.0",
    "copy-webpack-plugin": "^9.0.1",
    "cpx": "^1.5.0",
    "css-loader": "^6.2.0",
    "html-loader": "^2.1.2",
    "jest": "^27.0.6",
    "mini-css-extract-plugin": "^2.2.2",
    "nodemon": "^2.0.9",
    "sass": "^1.39.0",
    "sass-loader": "^12.1.0",
    "supertest": "^6.1.3",
    "ts-jest": "^27.0.3",
    "ts-loader": "^9.2.5",
    "typescript": "^4.3.5",
    "webpack": "^5.52.0",
    "webpack-cli": "^4.8.0"
  },
  "dependencies": {
    "amala": "^5.1.2",
    "axios": "^0.21.1",
    "axios-mock-adapter": "^1.19.0",
    "dotenv": "^10.0.0",
    "ect": "^0.5.9",
    "facebook-node-sdk": "^0.2.0",
    "jsonwebtoken": "^8.5.1",
    "koa": "^2.13.1",
    "koa-bodyparser": "^4.3.0",
    "koa-etag": "^4.0.0",
    "koa-router": "^10.0.0",
    "koa-static": "^5.0.0",
    "koa-views": "^7.0.1",
    "koahub-handlebars": "^2.0.9",
    "lodash": "^4.17.21",
    "module-alias": "^2.2.2",
    "sqlite3": "^5.0.2"
  },
  "_moduleAliases": {
    "@": "dist"
  }
}
